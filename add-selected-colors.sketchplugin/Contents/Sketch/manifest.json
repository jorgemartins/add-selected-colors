{
  "name": "Add Selected colors...",
  "description": "Sketch plugin to add colors from layers to Document palette (or JSON file)",
  "author": "Jorge Martins",
  "authorEmail": "hello@jorgemartins.me",
  "homepage": "https: //github.com/jorgemartins/add-selected-colors",
  "version": "0.1.0",
  "identifier": "me.jorgemartins.sketch.add-selected-colors",
  "appcast": "https://raw.githubusercontent.com/jorgemartins/add-selected-colors/master/.appcast.xml",
  "disableCocoaScriptPreprocessor": true,
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "icon": "add-selected-colors.png",
  "commands": [
    {
      "name": "to Document Colors",
      "identifier": "add-selected-colors.document-colors",
      "script": "my-command.js"
    },
    {
      "name": "to JSON file",
      "identifier": "add-selected-colors.file",
      "script": "my-command.js",
      "handler": "addJSON"
    },
    {
      "name": "Reset Palette",
      "identifier": "add-selected-colors.reset",
      "script": "my-command.js",
      "handler": "resetPalette"
    }
  ],
  "menu": {
    "title": "Add Selected Colors...",
    "items": [
      "add-selected-colors.document-colors",
      "add-selected-colors.file",
      "-",
      "add-selected-colors.reset"
    ]
  }
}